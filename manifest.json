{
    "name": "Yamp",
    "version": "0.0.1",
    "manifest_version": 2,

    "description": "Jamp from Yandex map in other",
    "icons": {"48":"images/mapicon.jpg"},

    "browser_action": {
        "default_title": "Jamp to ...",
        "default_icon": "images/mapicon.jpg",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "http://yandex.st/*",
        "http://ya.ru/"
    ],
    "background": { 
        "scripts": ["background.js"]    
    }
}
